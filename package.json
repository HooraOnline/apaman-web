{
  "name": "apaman-web",
  "version": "1.0.0",
  "private": true,
  "description": "Apaman reactJs, nextJs, I18N, MaterialUi init on Monta.ir",
  "scripts": {
    "dev": "node index.js",
    "build": "next build",
    "start": "next start -p 3000",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install && npm run dev",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",



    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:cypress": "NODE_ENV=test cypress open",
    "lint": "eslint .",
    "lint:watch": "esw . --watch --cache",
    "lint:watchAll": "esw . --watch",
    "circleci:run": "circleci local execute --job $JOB"
  },
  "keywords": [
    "next",
    "nextjs",
    "react",
    "reactjs",
    "npx",
    "material",
    "material-design",
    "material-kit",
    "material-kit-react",
    "next-material-kit"
  ],
  "author": "S.Mahdi Hejazi",
  "license": "MIT",
  "homepage": "https://www.monta.ir/apaman",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@next/bundle-analyzer": "^9.4.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "1.0.1",
    "accounting": "^0.4.1",
    "add": "^2.0.6",
    "classnames": "2.2.6",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "i18next": "^19.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "jss-rtl": "^0.3.0",
    "material-ui-popup-state": "^1.5.3",
    "mobx": "^5.15.1",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.1.4",
    "mobx-react-devtools": "^6.1.1",
    "moment": "2.24.0",
